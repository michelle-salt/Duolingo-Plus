{
  "name": "Duolingo Plus",
  "version": "1.0",
  "description": "Change website languages randomly",
  "manifest_version": 2,
  "permissions": ["https://en.wikipedia.org/*", "https://stackoverflow.com/*"],
  "content_scripts": [
    {
      "matches": ["https://en.wikipedia.org/*", "https://stackoverflow.com/*"],
      "js": ["dutch.js", "arabic.js", "greek.js", "japanese.js", "dictionary.js", "main.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
