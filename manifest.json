{
  "name": "Duolingo Plus",
  "version": "1.0",
  "description": "Change website languages randomly",
  "manifest_version": 2,
  "permissions": ["*://*/*"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["dutch.js", "arabic.js", "japanese.js", "dictionary.js", "main.js"], 
      "exclude_matches": [
        "https://www.google.com/*"
    ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
